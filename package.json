{
    "name": "lattice-grid",
    "version": "0.1.0",
    "private": true,
    "type": "commonjs",
    "dependencies": {
        "@dnd-kit/core": "^6.0.7",
        "@dnd-kit/modifiers": "^6.0.1",
        "@dnd-kit/sortable": "^7.0.2",
        "@dnd-kit/utilities": "^3.2.1",
        "@emotion/react": "^11.10.6",
        "@mantine/core": "^5.10.4",
        "@mantine/form": "^5.10.4",
        "@mantine/hooks": "^5.10.4",
        "@mantine/notifications": "^5.10.4",
        "@total-typescript/ts-reset": "^0.4.2",
        "@vitejs/plugin-react": "^3.1.0",
        "blockly": "^9.3.1",
        "js-base64": "^3.7.5",
        "lodash": "^4.17.21",
        "node-inspect-extracted": "^2.0.2",
        "pako": "^2.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.8.0",
        "react-router-dom": "^6.8.1",
        "valtio": "^1.10.2",
        "vite": "^4.1.2",
        "vite-plugin-svgr": "^2.4.0",
        "vitest": "^0.29.7"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.191",
        "@types/pako": "^2.0.0",
        "@types/react-dom": "^18.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "depcheck": "^1.4.5",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "eslint-plugin-testing-library": "^6.0.1",
        "eslint-plugin-valtio": "^0.6.2",
        "eslint-plugin-vitest": "^0.3.1",
        "fast-check": "^3.13.0",
        "pre-commit": "^1.2.2",
        "prettier": "^3.0.3",
        "prettier-plugin-organize-imports": "^3.2.3",
        "typescript": "^5.2.2"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "checkdeps": "yarn outdated; depcheck --ignores 'pre-commit,@emotion/react,prettier-plugin-organize-imports'",
        "precommit": "yarn typecheck; yarn test:once; yarn lint",
        "test": "vitest watch",
        "test:once": "vitest run --changed",
        "lint": "prettier --ignore-path .gitignore --check . && eslint --ignore-path .gitignore .",
        "lint:fix": "prettier --ignore-path .gitignore --write . && eslint --ignore-path .gitignore --fix .",
        "typecheck": "tsc --noEmit"
    },
    "pre-commit": [
        "typecheck",
        "test:once",
        "lint"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
