{
    "name": "lattice-grid",
    "version": "0.1.0",
    "private": true,
    "type": "commonjs",
    "dependencies": {
        "@dnd-kit/core": "^6.0.8",
        "@dnd-kit/modifiers": "^6.0.1",
        "@dnd-kit/sortable": "^7.0.2",
        "@dnd-kit/utilities": "^3.2.1",
        "@mantine/core": "^7.1.3",
        "@mantine/form": "^7.1.3",
        "@mantine/hooks": "^7.1.3",
        "@mantine/notifications": "^7.1.3",
        "@total-typescript/ts-reset": "^0.5.1",
        "@vitejs/plugin-react": "^4.1.0",
        "blockly": "^9.3.1",
        "clsx": "^2.0.0",
        "js-base64": "^3.7.5",
        "lodash": "^4.17.21",
        "node-inspect-extracted": "^3.0.0",
        "pako": "^2.0.4",
        "protobufjs": "~7.2.5",
        "protobufjs-cli": "~1.1.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.11.0",
        "react-router-dom": "^6.16.0",
        "valtio": "^1.11.2",
        "vite": "^4.4.11",
        "vite-plugin-svgr": "^4.1.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.199",
        "@types/pako": "^2.0.1",
        "@types/react-dom": "^18.2.13",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "depcheck": "^1.4.6",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "eslint-plugin-testing-library": "^6.1.0",
        "eslint-plugin-valtio": "^0.6.2",
        "eslint-plugin-vitest": "^0.3.2",
        "fast-check": "^3.13.1",
        "pre-commit": "^1.2.2",
        "prettier": "^3.0.3",
        "prettier-plugin-organize-imports": "^3.2.3",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6"
    },
    "scripts": {
        "dev": "vite",
        "build": "yarn protobuf && vite build",
        "checkdeps": "yarn outdated; depcheck --ignores 'pre-commit,@emotion/react,prettier-plugin-organize-imports'",
        "precommit": "yarn typecheck; yarn test:once; yarn lint",
        "test": "vitest watch",
        "test:once": "vitest run --changed",
        "lint": "prettier --ignore-path .gitignore --check . && eslint --ignore-path .gitignore .",
        "lint:fix": "prettier --ignore-path .gitignore --write . && eslint --ignore-path .gitignore --fix .",
        "protobuf": "yarn protobuf:js && yarn protobuf:ts",
        "protobuf:js": "pbjs -w es6 -t static-module src/encoding/latgrid.proto -o src/encoding/latgrid.js",
        "protobuf:ts": "pbts src/encoding/latgrid.js -o src/encoding/latgrid.d.ts",
        "typecheck": "tsc --noEmit"
    },
    "pre-commit": [
        "typecheck",
        "test:once",
        "lint"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
